{
  "name": "acarsd",
  "matches": [
    {
      "search": "headers[server]",
      "version": "(?-mix:^acarsd\\/([^\\s]+)$)"
    },
    {
      "certainty": 75,
      "text": "<meta name=\"author\" content=\"KjM <acarsd@acarsd.org>\">"
    },
    {
      "certainty": 75,
      "regexp": "(?-mix:<title>[^<]*RealTime Web ACARS)"
    },
    {
      "certainty": 75,
      "text": "<!-- MAIN PART OF WEBACARS -->"
    },
    {
      "string": "(?-mix:<meta name=\"description\" content=\"Realtime Web ACARS - [^\\s]+ Location: ([^\\.^\\\"^>]+)\\.)"
    }
  ],
  "dorks": [
    "intitle:\"RealTime Web ACARS\" \"ACARS decoder for Linux and Windows\" \"Help\" \"Map\" \"Reports\" \"Search\""
  ],
  "website": "http://www.acarsd.org/"
}